<%= form_for @club do |f| %>
  <div class="form-group">
  <%= f.label :team_name %>
  <%= f.text_field :team_name, :class => 'form-control' %>
  </div>
  <div class="form-group">
  <%= f.label :team_rank %>
  <%= f.text_field :team_rank, :class => 'form-control' %>
  </div>
  <div class="form-group">
  <%= f.label :manager %>
  <%= f.text_field :manager, :class => 'form-control' %>
  </div>
  <div class="form-group">
  <%= f.label :schedule %>
  <%= f.text_field :schedule, :class => 'form-control' %>
  </div>
  <div class="form-group">
  <%= f.label :image %>
  <%= f.url_field :image, :class => 'form-control' %>
  </div>
  <div class="form-group">
  <%= f.select :player_id, Player.pluck(:name, :id), :include_blank => true, :class => 'form-control' %>
  </div>
  <%= f.submit %>
<% end %>
